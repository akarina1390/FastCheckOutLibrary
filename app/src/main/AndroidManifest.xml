<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anakarinacarrocci.fastcheckoutlibrary">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.fastcheckoutlibrary.backend.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.fastcheckoutlibrary.backend.AdminView" />
        <activity android:name="com.fastcheckoutlibrary.backend.NewUserAccount" />
        <activity android:name="com.fastcheckoutlibrary.backend.SearchByAuthor" />
        <activity android:name="com.fastcheckoutlibrary.backend.SearchResults" />
        <activity android:name="com.fastcheckoutlibrary.backend.ListOfBooksResults" />
        <activity android:name="com.fastcheckoutlibrary.backend.UserSearchMainMenu" />
        <activity android:name="com.fastcheckoutlibrary.backend.SearchByBook" />
        <activity android:name="com.fastcheckoutlibrary.backend.SearchByPublisher" />
        <activity android:name="com.fastcheckoutlibrary.backend.SearchByLibrary"></activity>
    </application>

</manifest>