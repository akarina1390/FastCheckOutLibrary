<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anakarinacarrocci.fastcheckoutlibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminView" />
        <activity android:name=".NewUserAccount" />
        <activity android:name=".SearchByAuthor" />
        <activity android:name=".SearchResults" />
        <activity android:name=".ListOfBooksResults" />
        <activity android:name=".UserSearchMainMenu" />
        <activity android:name=".SearchByBook" />
        <activity android:name=".SearchByPublisher" />
        <activity android:name=".SearchByLibrary"></activity>
    </application>

</manifest>